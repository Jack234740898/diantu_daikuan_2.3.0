<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidthProgressBar" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundAngleImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="WaterRippleView">
        <attr name="wrv_radius" format="dimension" />
        <attr name="wrv_strokewidth" format="dimension" />
        <attr name="wrv_strokecolor" format="color" />
        <attr name="wrv_backgroundcolor" format="color" />
        <attr name="wrv_alpha" format="integer" />
        <attr name="wrv_circle_style">
            <enum name="FILL" value="0" />
            <enum name="STROKE" value="1" />
            <enum name="FILL_AND_STROKE" value="2" />
        </attr>
    </declare-styleable>
<declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
</resources>